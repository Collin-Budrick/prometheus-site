{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "storybook": "NODE_OPTIONS=--disable-warning=DEP0190 STORYBOOK_DISABLE_TELEMETRY=1 bunx storybook dev -p 6006 -c .storybook",
    "build": "bun run build:parallel",
    "build:parallel": "bun run scripts/build-parallel.ts",
    "prebuild": "bun run test:i18n",
    "preview": "bun run preview:fast",
    "preview:fast": "bun run scripts/preview-fast.ts",
    "lint": "bun run check:css && bun run check:scripts && oxlint .",
    "format": "oxfmt --write .",
    "test": "bun run test:unit && bun run test:e2e && bun run test:perf",
    "test:i18n": "bun test tests/home-i18n.test.ts",
    "test:unit": "bun test src tests/*.test.ts",
    "test:e2e": "bunx playwright test --config=playwright.config.ts --grep @smoke",
    "test:a11y": "bunx playwright test --config=playwright.config.ts --grep @a11y",
    "test:perf": "VITE_DEV_AUDIT=1 SKIP_PRERENDER=1 bunx playwright test --config=playwright.config.ts --grep @perf",
    "check:scripts": "bun run scripts/check-script-budget.ts",
    "check:css": "bun run scripts/check-css-budget.ts",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config drizzle.config.ts",
    "sync:page-config": "bun run scripts/sync-page-config.ts",
    "generate:uno": "bun run scripts/generate-uno.ts",
    "prerender": "TMPDIR=/tmp TEMP=/tmp TMP=/tmp bun x tsx scripts/prerender.ts"
  },
  "dependencies": {
    "@builder.io/qwik": "1.18.0",
    "@builder.io/qwik-city": "1.18.0",
    "@modular-forms/qwik": "0.29.1",
    "@qwik.dev/partytown": "0.11.2",
    "@unocss/reset": "66.5.10",
    "@unocss/runtime": "66.5.10",
    "@unocss/vite": "66.5.10",
    "compiled-i18n": "1.2.1",
    "drizzle-zod": "1.0.0-beta.5-e0482ac",
    "drizzle-orm": "1.0.0-beta.5-e0482ac",
    "motion": "13.0.0-alpha.0",
    "postgres": "3.4.7",
    "zod": "4.3.0-canary.20251223T202943"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@i18n/__data": "workspace:*",
    "@i18n/__locales": "workspace:*",
    "@i18n/__state": "workspace:*",
    "@iconify-json/solar": "1.2.5",
    "@playwright/test": "1.58.0-alpha-2025-12-24",
    "@rolldown/binding-win32-arm64-msvc": "1.0.0-beta.9-commit.d91dfb5",
    "@types/node": "25.0.3",
    "@unocss/preset-icons": "66.5.10",
    "@unocss/preset-mini": "66.5.10",
    "@unocss/preset-typography": "66.5.10",
    "@unocss/preset-wind": "66.5.10",
    "@unocss/transformer-directives": "66.5.10",
    "@unocss/transformer-variant-group": "66.5.10",
    "@vitejs/plugin-basic-ssl": "2.1.0",
    "bun-types": "1.3.6-canary.20251224T140738",
    "drizzle-kit": "1.0.0-beta.5-e0482ac",
    "ignore": "7.0.5",
    "lightningcss": "1.30.2",
    "lightningcss-win32-x64-msvc": "1.30.2",
    "oxfmt": "0.20.0",
    "oxlint": "1.35.0",
    "rollup-plugin-visualizer": "6.0.5",
    "storybook": "10.2.0-alpha.9",
    "storybook-framework-qwik": "^0.5.1--canary.e7aa5b7.0",
    "typescript": "6.0.0-dev.20251224",
    "unocss": "66.5.10",
    "vite": "8.0.0-beta.4",
    "vite-code-inspector-plugin": "1.0.5",
    "vite-plugin-checker": "0.12.0",
    "vite-plugin-compression2": "2.4.0",
    "vite-plugin-inspect": "11.3.3",
    "vite-plugin-preload": "0.4.4",
    "vite-plugin-pwa": "1.2.0",
    "vite-plugin-static-copy": "3.1.4",
    "vite-tsconfig-paths": "6.0.3"
  }
}
