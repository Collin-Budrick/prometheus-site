{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run scripts/dev.ts",
    "build": "bun run generate:uno && bun run check:css && vite build && vite build --ssr && bun run prerender && bun run scripts/emit-locale-build-dirs.ts en ko",
    "preview": "bun run preview:fast",
    "preview:fast": "bun run scripts/preview-fast.ts",
    "lint": "bun run check:css && bun run check:scripts && oxlint .",
    "format": "oxfmt --write .",
    "test": "bun run test:unit && bun run test:e2e && bun run test:perf",
    "test:unit": "bun test src",
    "test:e2e": "bunx playwright test --config=playwright.config.ts --grep @smoke",
    "test:perf": "bunx playwright test --config=playwright.config.ts --grep @perf",
    "check:scripts": "bun run scripts/check-script-budget.ts",
    "check:css": "bun run scripts/check-css-budget.ts",
    "generate:uno": "unocss \"src/**/*.{ts,tsx,js,jsx,mdx,md}\" \"index.html\" -c uno.config.ts -o public/assets/app.css -m",
    "prerender": "TMPDIR=/tmp TEMP=/tmp TMP=/tmp bunx tsx scripts/prerender.ts"
  },
  "dependencies": {
    "@builder.io/qwik": "1.18.0",
    "@builder.io/qwik-city": "1.18.0",
    "@qwik.dev/partytown": "0.11.2",
    "@unocss/reset": "66.5.10",
    "@unocss/runtime": "66.5.10",
    "@unocss/vite": "66.5.10",
    "compiled-i18n": "1.2.1",
    "drizzle-orm": "1.0.0-beta.3-d4ff358",
    "motion": "13.0.0-alpha.0",
    "postgres": "3.4.7"
  },
  "devDependencies": {
    "@iconify-json/solar": "1.2.5",
    "@playwright/test": "1.58.0-alpha-2025-12-19",
    "@types/node": "25.0.3",
    "@unocss/preset-icons": "66.5.10",
    "@unocss/preset-mini": "66.5.10",
    "@unocss/preset-typography": "66.5.10",
    "@unocss/preset-wind": "66.5.10",
    "@unocss/transformer-directives": "66.5.10",
    "@unocss/transformer-variant-group": "66.5.10",
    "@vitejs/plugin-basic-ssl": "2.1.0",
    "bun-types": "1.3.6-canary.20251219T140743",
    "ignore": "7.0.5",
    "lightningcss": "1.30.2",
    "oxfmt": "0.19.0",
    "oxlint": "1.34.0",
    "rollup-plugin-visualizer": "6.0.5",
    "typescript": "6.0.0-dev.20251219",
    "unocss": "66.5.10",
    "vite": "8.0.0-beta.3",
    "vite-plugin-inspect": "11.3.3",
    "vite-plugin-pwa": "1.2.0",
    "vite-tsconfig-paths": "6.0.3"
  }
}
