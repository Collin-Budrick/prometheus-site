    "@unocss/vite": "66.5.10",
    "compiled-i18n": "1.2.1",
    "drizzle-orm": "1.0.0-beta.3-d4ff358",
    "motion": "^13.0.0-alpha.0",
    "postgres": "3.4.7"
  },
